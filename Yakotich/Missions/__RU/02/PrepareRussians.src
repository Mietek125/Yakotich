Michail := PrepareUnit('Michail', false, '');
Vasilij := PrepareUnit('Vasilij', false, '');
Maksim := PrepareUnit('Maksim', false, '');
Fiodor := PrepareUnit('Fiodor', false, '');

Uc_Side := 5;

Aleksandr := PrepareUnit('Aleksandr', false, '');

end;









